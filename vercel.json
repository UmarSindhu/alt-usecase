{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/sitemap.xml",
      "dest": "/api/sitemap",
      "headers": {
        "Content-Type": "text/xml"
      }
    }
  ]
}
